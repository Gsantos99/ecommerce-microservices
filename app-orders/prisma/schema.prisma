datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


generator client {
  provider = "prisma-client-js"
}


enum OrderStatus {
  PENDING
  PAID
  CANCELLED
}

model Order {
  id          String   @id @default(cuid())
  customerId  String
  status      OrderStatus @default(PENDING)
  createdAt   DateTime @default(now())
  amount      Int
}


